{
  "name": "eslint-plugin-smart-apostrophes",
  "version": "0.3.1",
  "description": "Prefer the use of smart apostrophes over straight ones. Supports `.js`, `.ts` and `.vue` files.",
  "main": "./dist/index.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --no-splitting --minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/younesaassila/eslint-plugin-smart-apostrophes.git"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "quotes",
    "smart-quotes",
    "smart-quotes-only"
  ],
  "author": "Younes Aassila",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/younesaassila/eslint-plugin-smart-apostrophes/issues"
  },
  "homepage": "https://github.com/younesaassila/eslint-plugin-smart-apostrophes#readme",
  "devDependencies": {
    "@types/eslint": "^8.2.0",
    "@types/estree": "0.0.50",
    "@types/node": "^16.11.9",
    "tsup": "^5.9.0",
    "typescript": "^4.5.2",
    "vue-eslint-parser": "^8.0.1"
  }
}
